
@{
    ViewBag.Title = "Dashboard";
}
</head>
<div class="container">
    <div class="col-12">
        <!-- Content Header (Page header) -->
        <div class="content-header">
            <div class="container-fluid">
                <div class="row mb-2">
                    <div class="col-sm-6">
                        <h1 class="m-0 text-dark">Dashboard</h1>
                    </div><!-- /.col -->
                    <div class="col-sm-6">
                        <ol class="breadcrumb float-sm-right">
                            <li class="breadcrumb-item"><a href="#">Home</a></li>
                            <li class="breadcrumb-item active">Phage</li>
                        </ol>
                    </div><!-- /.col -->
                </div><!-- /.row -->
            </div><!-- /.container-fluid -->
        </div>
        <div class="row">
            <div class="clearfix hidden-md-up"></div>
            @if (User.IsInRole("NewUser"))
            {
                @*<div class="col-12 col-sm-6 col-md-3">
                <a class="nav-link disabled" href="/Projects/ManageProjects">
                    <div class="info-box mb-3" id="ticketbox">
                        <span class="info-box-icon bg-gradient-gray-dark elevation-1"><i class="fas fa-project-diagram"></i></span>
                        <div class="info-box-content">
                            <span class="info-box-text" id="tbutt">My Projects</span>
                            <span class="info-box-number"></span>
                        </div>
                         /.info-box-content
                    </div>
                     /.info-box
                </a>
            </div>*@
                <div class="col-md-6">
                    <div class="card">
                        <div class="card-header">
                            <h5 class="card-title m-0">WELCOME!</h5>
                        </div>
                        <div class="card-body">
                            <h6 class="card-title">Once you are assigned a role, you will have access to the features of that role. </h6>

                            <p class="card-text"></p>
                            <a href="@Url.Action("Create","Households")" class="btn accent-secondary">Create</a>
                        </div>
                    </div>
                </div>
            }
            else
            {
                <div class="col-12 col-sm-6 col-md-3">
                    <a class="nav-link" href="/Projects/ManageProjects">
                        <div class="info-box mb-3" id="ticketbox">
                            <span class="info-box-icon bg-gradient-gray-dark elevation-1"><i class="fas fa-project-diagram"></i></span>
                            <div class="info-box-content">
                                <span class="info-box-text" id="tbutt">My Projects</span>
                                <span class="info-box-number"></span>
                            </div>
                            <!-- /.info-box-content -->
                        </div>
                        <!-- /.info-box -->
                    </a>
                </div>

            }

            <div class="col-12 col-sm-6 col-md-3">
                @if (User.IsInRole("Admin") || User.IsInRole("PM") || User.IsInRole("DemoPM") || User.IsInRole("DemoAdmin"))
                {
                    <a class="nav-link" href="/Projects/Create">
                        <div class="info-box" id="ticketbox">
                            <span class="info-box-icon bg-gradient-dark elevation-1"><i class="fas fa-pen"></i></span>
                            <div class="info-box-content">
                                <span class="info-box-text" id="tbutt">Create New Project</span>
                                <span class="info-box-number">
                                </span>
                            </div>
                            <!-- /.info-box-content -->
                        </div>
                        <!-- /.info-box -->
                    </a>
                }
            </div>
            @if (User.IsInRole("Admin") || User.IsInRole("PM") || User.IsInRole("DemoPM") || User.IsInRole("DemoAdmin"))
            {
                <!-- /.col -->
                <div class="col-12 col-sm-6 col-md-3">
                    <a class="nav-link" href="/Projects/ListView">
                        <div class="info-box mb-3" id="ticketbox">
                            <span class="info-box-icon bg-gradient-gray-dark elevation-1"><i class="fas fa-list-alt"></i></span>
                            <div class="info-box-content">
                                <span class="info-box-text" id="tbutt">All Projects</span>
                                <span class="info-box-number"></span>
                            </div>
                            <!-- /.info-box-content -->
                        </div>
                    </a>
                    <!-- /.info-box -->
                </div>
